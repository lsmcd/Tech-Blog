<form id="updateblog">
    <div class="field">
        <label class="label">Title</label>
        <div class="control">
            <input id="title" class="input" type="text" value="{{title}}">
        </div>
    </div>

    <div class="field">
        <label class="label">Content</label>
        <div class="control">
            <input id="content" class="input" type="text" value="{{content}}">
        </div>
    </div>

    <div class="field">
        <div class="control">
            <button class="button">Update</button>
        </div>
    </div>
</form>
<button class="button mt-3" id="delete">Delete</button>
<script src="/js/updateBlog.js"></script>